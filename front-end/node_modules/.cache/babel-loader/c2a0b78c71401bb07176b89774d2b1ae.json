logFormVisible = false\n        },\n        cancel(){\n            this.resetForm()\n            this.dialogFormVisible = false\n        },\n        resetForm(){\n            this.form = {\n                user:\"\",\n                password:\"\",\n                role:\"user\"\n            }\n        }\n    },\n    mounted:function() {\n        this.getData()\n        setInterval(this.getData(),2000)\n    },\n    props:{\n        height:{\n            default:270\n        }\n    }\n};\n</script>\n\n<style scoped>\n.ms-content {\n    padding: 30px 30px 10px 30px;\n}\n\n.btn{\n    width:100%\n}\n\na{\n    color: #3498db;\n}\n\n.button-custom{\n    padding:0px;\n    border: 0px;\n}\n</style>\n\n"],"sourceRoot":"src/components/page"}]}