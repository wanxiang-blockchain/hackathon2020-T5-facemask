ceipt\":{\n                    \"contract_address\":\"\", \n                    \"status\":1, \n                    \"event\":\"\", \n                    \"gas_used\":0, \n                }\n            }\n        } \n     },\n    props: ['txHash'],\n    watch: {\n        '$route' (to, from) {\n            this.getData()\n        }\n    },\n    methods:{\n        getData(){\n            this.axios.get(dataUrl + '/tx/' + this.txHash)\n            .then(res => {\n                this.tx = res.data;\n            }).catch(res => {\n                alert('wrong txdetail');\n            });\n        }\n    },\n    computed:{\n        items : [\n            {\n                id:1,\n                content:\"这是一段内容\",\n                field:\"Transaction Hash:\",\n                value:this.tx.tx_hash\n            },\n            {\n                id:2,\n                content:\"这是一段内容2\",\n                field:\"Status:\",\n                value:this.tx.receipt.status\n            },\n            {\n                id:3,\n                content:\"这是一段内容3\",\n                field:\"Block:\",\n                value:this.tx.block_height\n            },\n            {\n                id:4,\n                content:\"这是一段内容4\",\n                field:\"Timestamp:\",\n                value:(new Date(this.tx.timestamp * 1000)).toLocaleString()\n            },\n            {\n                id:5,\n                content:\"这是一段内容5\",\n                field:\"From:\",\n                value:this.tx.from\n            },\n            {\n                id:6,\n                content:\"这是一段内容6\",\n                field:\"To:\",\n                value:this.tx.to\n            },\n            {\n                id:7,\n                content:\"这是一段内容7\",\n                field:\"Value:\",\n                value:this.tx.value\n            },\n            {\n                id:8,\n                content:\"这是一段内容9\",\n                field:\"Gas Used:\",\n                value:this.tx.receipt.gas_used\n            },\n            {\n                id:9,\n                content:\"这是一段内容8\",\n                field:\"Gas Limit:\",\n                value:this.tx.gas_limit\n            },\n            {\n                id:10,\n                content:\"这是一段内容10\",\n                field:\"Gas Price:\",\n                value:this.tx.gas_price\n            },\n            {\n                id:11,\n                content:\"这是一段内容11\",\n                field:\"Nonce:\",\n                value:this.tx.nonce\n            },\n            {\n                id:12,\n                content:\"这是一段内容12\",\n                field:\"Input Data:\",\n                value:this.tx.nonce\n            }\n        ]\n    },\n    mounted:function() {\n        this.getData()\n    }\n}\n</script>\n<style scoped>\n.title_bn{\n    color:#6c757e;\n    line-height:1.7;\n    margin-top:10px;\n    margin-left:10px;\n}\n.card_content{\n    margin-top:20px;\n    margin-right:10px;\n}\n.hr_space{\n    margin-top: .75rem;\n    margin-bottom: .75rem;\n    opacity: .75;\n    border-top:1px solid #e7eaf3;\n    border-bottom:0px;\n    border-left: 0px;\n    border-right: 0px;\n}\n.icon-li-content{\n    display: flex;\n    height: 100%;\n    flex-direction: column;\n    align-items: center;\n    justify-content: center;\n    cursor: pointer;\n}\n\n.tx-status{\n    color:#00c9a7;\n    background-color:rgba(0,201,167,.1);\n    font-size: small;\n    width:70px;\n    height:22px;\n    text-align: center;\n    border-radius: 3px;\n}\n\n.tx-fail{\n    color:#de4437;\n    background-color: rgba(222,68,55,.1);\n}\n\na{\n    color: #3498db;\n}\n\n.tx-detail >>> .el-progress-bar__outer{\n    display:block;\n    width:400px;\n}\n</style>"]}]}